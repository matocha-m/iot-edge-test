{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "SampleModule Local Debug (java)",
      "request": "launch",
      "cwd": "${workspaceRoot}/modules/SampleModule",
      "console": "internalConsole",
      "stopOnEntry": false,
      "mainClass": "com.edgemodule.App",
      "args": "",
      "projectName": "SampleModule",
      "env": {
        "EdgeHubConnectionString": "${config:azure-iot-edge.EdgeHubConnectionString}",
        "EdgeModuleCACertificateFile": "${config:azure-iot-edge.EdgeModuleCACertificateFile}"
      }
    },
    {
      "type": "java",
      "name": "SampleModule Remote Debug (java)",
      "request": "attach",
      "hostName": "localhost",
      "port": 5005
    }
  ]
}